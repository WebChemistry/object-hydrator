{
    "name": "webchemistry/object-hydrator",
    "description": "",
    "license": "proprietary",
    "type": "library",
    "require": {
        "php": ">=7.4",
        "symfony/polyfill-php80": "*",
        "symfony/serializer": "^5.0",
        "symfony/property-access": "^5.0",
        "symfony/property-info": "^5.0",
        "symfony/validator": "^5.0"
    },
    "require-dev": {
        "codeception/codeception": "^4.1",
        "codeception/module-asserts": "^1.1",
        "phpstan/phpstan": "^0.12.11",
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan-deprecation-rules": "^0.12.2",
        "warengo/qa": "2.0.1",
        "codeception/module-phpbrowser": "^1.0.0"
    },
    "autoload": {
        "psr-4": {
            "WebChemistry\\ObjectHydrator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WebChemistry\\ObjectHydrator\\Testing\\": "tests/src/"
        }
    },
    "scripts": {
        "check": [
            "@cs",
            "@phpstan",
            "@tests"
        ],
        "tests": "vendor/bin/codecept run",
        "phpstan": "vendor/bin/phpstan analyse -l 8 -c phpstan.neon src",
        "cs": "vendor/bin/codesniffer",
        "cs-fix": "vendor/bin/codefixer"
    }
}
